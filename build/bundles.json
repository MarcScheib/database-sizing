{
  "bundles": {
    "dist/app-build": {
      "includes": [
        "[*]",
        "*.html!text",
        "*.css!text"
      ],
      "options": {
        "inject": true,
        "minify": true
      }
    },
    "dist/aurelia": {
      "includes": [
        "aurelia-bootstrapper",
        "aurelia-framework",
        "jquery",
        "bootstrap"
      ],
      "options": {
        "inject": true,
        "minify": true
      }
    }
  }
}
