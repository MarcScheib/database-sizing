{
  "bundles": {
    "dist/app-build": {
      "includes": [
        "[**/*.js]",
        "*.html!text"
      ],
      "options": {
        "inject": true,
        "minify": true,
        "depCache": false,
        "rev": true
      }
    },
    "dist/aurelia": {
      "includes": [
        "aurelia-animator-css",
        "aurelia-bootstrapper",
        "aurelia-framework",
        "aurelia-loader-default",
        "aurelia-logging-console",
        "aurelia-templating-binding",
        "aurelia-templating-resources"
      ],
      "options": {
        "inject": true,
        "minify": true,
        "depCache": false,
        "rev": true
      }
    },
    "dist/vendor": {
      "includes": [
        "jquery",
        "bootstrap"
      ],
      "options": {
        "inject": true,
        "minify": false,
        "depCache": true,
        "rev": true
      }
    }
  }
}
